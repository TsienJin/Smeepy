{
  "name": "paddock",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:build\" \"npm run dev:start\"",
    "dev:build": "swc src --ignore ./**/*.test.ts -w --out-dir dist",
    "dev:start": "NODE_ENV=dev nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "test": "jest",
    "build": "swc src --ignore ./**/*.test.ts -d dist",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "Ong Tsien Jin",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.87",
    "@swc/jest": "^0.2.29",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.3",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
