{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true,
    },
    "target": "es2020",
    "paths": {
      "@src/*": ["./src/*"],
      "@routes/*": ["./src/routes/*"],
      "@middleware/*": ["./src/middleware/*"],
      "@beaver/*": ["./src/routes/beaver/*"]
    },
    "baseUrl": "."
  },
  "exclude": [
    ".test.ts"
  ],
  "module": {
    "type": "commonjs"
  },
  "minify": true
}