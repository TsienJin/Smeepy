{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@src/*": ["./src/*"],
      "@routes/*": ["./src/routes/*"]
    },
    "baseUrl": "."
  },
  "exclude": [
    ".test.ts"
  ],
  "module": {
    "type": "commonjs"
  }
}