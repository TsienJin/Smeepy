{
  "name": "paddock",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:build\" \"npm run dev:start\"",
    "dev:build": "swc src -w --out-dir dist",
    "dev:start": "dotenv -e .env.dev -- nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "test": "jest",
    "prisma:dev": "npm run prisma:dev:generate && npm run prisma:dev:migrate",
    "prisma:dev:generate": "npx prisma generate",
    "prisma:dev:migrate": "dotenv -e .env.dev -- npx prisma migrate dev",
    "build": "swc src -d dist",
    "start": "node dist/index.js"
  },
  "keywords": [],
  "author": "Ong Tsien Jin",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.87",
    "@swc/jest": "^0.2.29",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.3",
    "@types/ws": "^8.5.7",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/uuid": "^9.0.4",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yup": "^1.3.1"
  }
}
